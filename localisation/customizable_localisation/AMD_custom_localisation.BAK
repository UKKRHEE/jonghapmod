defined_text = {	name = AMD_CLK_PLACEHOLDER0		#완전 범용. 사용 자제.
	text = {	localisation_key = AMD_EVTDESC_ERROR
		trigger = {
			event_target:AMD_ET_POV = {
				OR = {
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_DOABLE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_DOABLE
				}
			}
		}
	}
	text = {	localisation_key = AMD_EVTDESC_2P
		trigger = {
			event_target:AMD_ET_POV = {
				NOT = {
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_DOABLE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_DOABLE
				}
			}
			AMD_T_STF_IS_2P = yes
			AMD_T_STF_IS_ERA = no
		}
	}
	text = {	localisation_key = AMD_EVTDESC_ORGY
		trigger = {
			event_target:AMD_ET_POV = {
				NOT = {
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_DOABLE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_DOABLE
				}
			}
			AMD_T_STF_IS_2P = no
			AMD_T_STF_IS_ORGY = yes
		}
	}
	text = {	localisation_key = AMD_EVTDESC_ERA
		trigger = {
			event_target:AMD_ET_POV = {
				NOT = {
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_ALIVE
					has_character_flag = AMD_CF_ERROR_ATK_IS_NOT_DOABLE
					has_character_flag = AMD_CF_ERROR_DEF_IS_NOT_DOABLE
				}
			}
			AMD_T_STF_IS_ERA = yes
		}
	}
}

